generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        Int     @id @default(autoincrement())
  name      String  @default("Raphael")
  username  String  @unique @default("raphaelhozj@gmail.com")
  password  String  @default("1234567")
}


model Session {
  id        String   @id
  sid       String   @unique
  data      String   @db.Text // contains session data
  expiresAt DateTime 
}